{
  "manifest_version": 1,
  "name": "bilibili_dynamic_push_plugin",
  "version": "1.1.0",
  "description": "定时检测B站UP主动态并推送到指定QQ群（跳过置顶、支持多UID/多群、启动日志提示）。",
  "author": {
    "name": "白狐",
    "url": "https://github.com/whitefox20011120/bilibili_dynamic_push_plugin"
  },
  "license": "MIT",
  "host_application": {
    "min_version": "0.9.0",
    "max_version": "0.9.1"
  },
  "keywords": ["bilibili", "动态", "自动推送", "UP主", "QQ"],
  "categories": ["automation", "bilibili", "push"],
  "repository_url": "",
  "default_locale": "zh-CN",
  "plugin_info": {
    "is_built_in": false,
    "components": [
      {
        "type": "action",
        "name": "auto_push",
        "description": "自动监控UP主动态并推送到QQ群"
      }
    ]
  },
  "python_dependencies": ["aiohttp"]
}
