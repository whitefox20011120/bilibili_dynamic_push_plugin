{
  "manifest_version": 1,
  "name": "bilibili_dynamic_push",
  "version": "1.0.0",
  "description": "基于MaiBot的B站动态订阅插件，支持多UP主、多群组推送，自动解析图文、视频封面及转发动态",
  "author": {
    "name": "白狐",
    "url": ""
  },
  "license": "MIT",
  "host_application": {
    "min_version": "0.10.0"
  },
  "keywords": [
    "bilibili",
    "动态订阅",
    "推送",
    "监控",
    "UP主"
  ],
  "categories": [
    "automation",
    "entertainment",
    "utility"
  ],
  "repository_url": "",
  "default_locale": "zh-CN",
  "plugin_info": {
    "is_built_in": false,
    "components": [
      {
        "type": "command",
        "name": "bili_control",
        "description": "B站订阅插件"
      }
    ]
  },
  "python_dependencies": [
    "bilibili-api-python>=16.2.0",
    "aiohttp"
  ],
  "id": "MaiBotUser.bilibili_dynamic_subscription"
}
