{
  "manifest_version": 1,
  "name": "B站动态推送插件",
  "version": "1.3.0",
  "description": "定时检测B站UP主动态并推送到指定QQ群（跳过置顶、支持多UID/多群、启动日志提示）。",
  "author": {
    "name": "白狐",
    "url": "https://github.com/whitefox20011120"
  },
  "license": "AGPL-3.0",
  "host_application": {
    "min_version": "0.10.0",
    "max_version": "0.10.1"
  },
  "homepage_url": "https://github.com/whitefox20011120/bilibili_dynamic_push_plugin",
  "repository_url": "https://github.com/whitefox20011120/bilibili_dynamic_push_plugin",
  "config_file_name": "config.toml",
  "keywords": [
    "bilibili",
    "动态",
    "自动推送",
    "UP主",
    "QQ"
  ],
  "categories": [
    "automation",
    "bilibili",
    "push"
  ],
  "repository_url": "",
  "default_locale": "zh-CN",
  "plugin_info": {
    "is_built_in": false,
    "components": [
      {
        "type": "action",
        "name": "auto_push",
        "description": "自动监控UP主动态并推送到QQ群"
      }
    ]
  },
  "python_dependencies": [
    "aiohttp"
  ]
}
